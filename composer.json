{
  "name": "ryantao/api-response",
  "description": "快速简单的多通道记录日志",
  "keywords": [
    "log",
    "logger",
    "laravel",
    "thinkphp",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "monolog/monolog": "2.9.2",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "ryantao\\api-response\\": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "cp -r src/install/Library %project_root%"
    ],
    "post-update-cmd": [
      "cp -r src/install/Library %project_root%"
    ]
  },
  "extra": {
    "think": {
      "config": {
        "rlog": "src/install/config/rlog.php"
      }
    }
  },
  "authors": [
    {
      "name": "Ryan",
      "email": "11204356@qq.com"
    }
  ]
}
