{
  "name": "ryantao/api-response",
  "description": "快速简单的多通道记录日志",
  "keywords": [
    "log",
    "logger",
    "laravel",
    "thinkphp",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2.0",
    "monolog/monolog": "2.9.*",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "post-install-cmd": [
      "cp -r ./vendor/ryantao/api-response/src/config/rlog.php ./config/rlog.php",
      "cp -r ./vendor/ryantao/api-response/src/Logger/RLog.php ./extend/RLog.php"
    ],
    "post-update-cmd": [
      "cp -r ./vendor/ryantao/api-response/src/config/rlog.php ./config/rlog.php",
      "cp -r ./vendor/ryantao/api-response/src/Logger/RLog.php ./extend/RLog.php"
    ]
  },
  "autoload": {
    "psr-4": {
      "Ryantao\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ryan",
      "email": "11204356@qq.com"
    }
  ]
}
